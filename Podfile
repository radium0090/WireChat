  pod 'Firebase/Storage'
  pod 'Firebase/Firestore'

  target 'WireChatTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WireChatUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
